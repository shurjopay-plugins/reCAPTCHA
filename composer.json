{
    "name": "shurjomukhi/no-captcha",
    "description": "No CAPTCHA reCAPTCHA For Laravel.",
    "keywords": [
        "recaptcha",
        "no-captcha",
        "captcha",
        "laravel",
        "laravel4",
        "laravel5",
        "laravel6"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "shurjomukhi",
            "email": "shurjomukhi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "shurjomukhi\\NoCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "shurjomukhi\\NoCaptcha\\NoCaptchaServiceProvider"
            ],
            "aliases": {
                "NoCaptcha": "shurjomukhi\\NoCaptcha\\Facades\\NoCaptcha"
            }
        }
    }
}
